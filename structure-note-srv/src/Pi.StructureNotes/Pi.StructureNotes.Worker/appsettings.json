{
    "AwsSqs": {
        "Region": "ap-southeast-1"
    },
    "RemoteConfig": {
        "Enable": true,
        "LifetimeMS": 86400000,
        "Prefix": "structurenotes-worker"
    },
    "MassTransit": {
        "EndpointNamePrefix": "StructureNotes-srv"
    },
    "EnableDbSensitiveDataLogging": false,
    "OpenTelemetry": {
        "Exporter": {
            "Otlp": {
                "Enabled": true,
                "Endpoint": "http://prometheus.pi.financial.prod.internal/api/v1/otlp/v1/metrics",
                "TracingEndpoint": "http://datadog-agent.datadog.svc.cluster.local:4317"
            },
            "Console": {
                "Enabled": false
            }
        }
    }
}
