{
  "AwsSqs": {
    "Region": "ap-southeast-1"
  },
  "RemoteConfig": {
    "Enable": true,
    "LifetimeMS": 86400000,
    "Prefix": "pi/tfex-srv"
  },
  "MassTransit": {
    "EndpointNamePrefix": "tfex-srv"
  },
  "EnableDbSensitiveDataLogging": false,
  "OpenTelemetry": {
    "Exporter": {
      "Otlp": {
        "Enabled": true,
        "Endpoint": "http://prometheus.pi.financial.prod.internal/api/v1/otlp/v1/metrics",
        "TracingEndpoint": "http://datadog-agent.datadog.svc.cluster.local:4317"
      },
      "Console": {
        "Enabled": false
      }
    }
  },
  "SetTrade": {
    "Host": "https://open-api.settrade.com",
    "BrokerId": "003",
    "Application": "ADMIN",
    "ApiKey": "xx",
    "AppSecret": "xx",
    "Timeout": 5,
    "AuthMaxRetry": 1
  },
  "SetTradeStream": {
    "Path": "/api/dispatcher/v3/003/mqtt?openapi_token=",
    "Topic": [
      "proto/bs/_broker/S/orderdvv3",
      "proto/bs/_broker/F/orderdvv3",
      "proto/bs/_broker/D/orderdvv3"
    ],
    "ConnectionDelaySeconds": 5,
    "HealthCheckSeconds": 300
  },
  "OperationHours": {
    "Start": "06:00",
    "End": "18:00"
  },
  "Features": {
    "IsTfexListenerNotificationEnabled": true
  },
  "Redis": {
    "Enabled": true,
    "Ssl": true,
    "Host": "master.pi-redis-nonprod.pbikfr.apse1.cache.amazonaws.com",
    "Port": 6379,
    "Database": 1,
    "Username": "tfex-srv",
    "Password": "",
    "ConnectTimeout": 5000,
    "ConnectRetry": 3,
    "SyncTimeout": 5000,
    "AbortOnConnectFail": false,
    "ClientName": "tfex-srv"
  }
}
