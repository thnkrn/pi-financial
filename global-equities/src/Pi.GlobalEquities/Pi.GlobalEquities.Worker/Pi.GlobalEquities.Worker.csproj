<Project Sdk="Microsoft.NET.Sdk.Worker">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>Pi.GlobalEquities.Worker</RootNamespace>
    <DockerComposeProjectPath>../docker-compose.dcproj</DockerComposeProjectPath>
    <UserSecretsId>f437caf7-d339-4666-8080-fdeced2d8770</UserSecretsId>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <NoWarn>1701;1702;1591</NoWarn>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <NoWarn>1701;1702;1591</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="EFCore.NamingConventions" Version="8.0.3" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="8.0.1" />
    <PackageReference Include="MassTransit" Version="8.0.15" />
    <PackageReference Include="MassTransit.EntityFrameworkCore" Version="8.0.15" />
    <PackageReference Include="MassTransit.Analyzers" Version="8.0.15" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="7.0.0" />
    <PackageReference Include="MongoDB.Bson" Version="3.1.0" />
    <PackageReference Include="MongoDB.Driver" Version="3.1.0" />
    <PackageReference Include="Pi.Client.NotificationService" Version="1.0.8" />
    <PackageReference Include="Pi.Client.UserSrvV2" Version="1.0.103" />
    <PackageReference Include="Pi.Common.Serilog" Version="1.0.121" />
    <PackageReference Include="pi.common.startup" Version="1.0.121" />
    <PackageReference Include="Pomelo.EntityFrameworkCore.MySql" Version="8.0.2" />
    <PackageReference Include="MassTransit.AmazonSQS" Version="8.0.15" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="7.0.1">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Amazon.Extensions.Configuration.SystemsManager" Version="6.2.2" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="MassTransit" />
    <None Remove="MassTransit.EntityFrameworkCore" />
    <None Remove="MassTransit.AmazonSQS" />
    <None Remove="MassTransit.Analyzers" />
    <None Remove="MassTransit.RabbitMQ" />
    <None Remove="Microsoft.Extensions.Http" />
    <None Remove="Serilog.Extensions.Hosting" />
    <None Remove="Serilog.Sinks.Console" />
    <None Remove="Serilog.Settings.Configuration" />
    <None Remove="Serilog.Sinks.Seq" />
    <None Remove="Pomelo.EntityFrameworkCore.MySql" />
    <None Remove="Startup\" />
    <None Remove="Microsoft.EntityFrameworkCore.Design" />
    <None Remove="Amazon.Extensions.Configuration.SystemsManager" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Pi.GlobalEquities.Repositories\Pi.GlobalEquities.Repositories.csproj" />
    <ProjectReference Include="..\Pi.GlobalEquities.Services\Pi.GlobalEquities.Services.csproj" />
  </ItemGroup>
</Project>
