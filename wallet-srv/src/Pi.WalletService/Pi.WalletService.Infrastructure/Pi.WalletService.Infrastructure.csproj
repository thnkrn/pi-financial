<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>Pi.WalletService.Infrastructure</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <NoWarn>1701;1702;1591</NoWarn>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <NoWarn>1701;1702;1591;CS0618;IL2121</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Pi.Client.CgsBank\Pi.Client.CgsBank.csproj" />
    <ProjectReference Include="..\Pi.WalletService.Application\Pi.WalletService.Application.csproj" />
    <ProjectReference Include="..\Pi.WalletService.Domain\Pi.WalletService.Domain.csproj" />
    <ProjectReference Include="..\Pi.Client.ExanteUserManagement\Pi.Client.ExanteUserManagement.csproj" />
    <ProjectReference Include="..\Pi.Client.ExanteTrade\Pi.Client.ExanteTrade.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="EntityFrameworkCore.DataEncryption" Version="4.0.0" />
    <PackageReference Include="MassTransit.AmazonSQS" Version="8.0.15" />
    <PackageReference Include="MassTransit.EntityFrameworkCore" Version="8.0.15" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.11" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="7.0.3" />
    <PackageReference Include="Pi.Client.EmployeeService" Version="1.0.3" />
    <PackageReference Include="Pi.Client.NotificationService" Version="1.0.8" />
    <PackageReference Include="pi.client.paymentservice" Version="1.0.5" />
    <PackageReference Include="Pi.Client.Sirius" Version="1.0.33" />
    <PackageReference Include="pi.client.onboardservice" Version="1.0.31" />
    <PackageReference Include="Pi.Client.UserService" Version="1.0.36" />
    <PackageReference Include="Pi.Common.Database" Version="1.0.86" />
    <PackageReference Include="Pi.Financial.Client.Freewill" Version="1.0.67" />
    <PackageReference Include="pi.financial.client.settrade" Version="1.0.7" />
    <PackageReference Include="Pomelo.EntityFrameworkCore.MySql" Version="7.0.0" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.27.0" />
    <PackageReference Include="Pi.Common" Version="1.0.86" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
    <PackageReference Include="Pi.Client.FxService" Version="1.0.10" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Pi.Common" />
    <None Remove="Options\" />
    <None Remove="EntityConfigs\" />
  </ItemGroup>
</Project>