{
  "RemoteConfig": {
    "Enable": true
  },
  "MassTransit": {
    "EndpointNamePrefix": "backoffice-srv-staging"
  },
  "Cors": {
    "Origins": [
      "http://localhost:3000"
    ]
  },
  "WalletService": {
    "Host": "http://wallet-api.wallet.svc.cluster.local"
  },
  "OnboardService": {
    "Host": "http://onboard-api.onboard.svc.cluster.local"
  },
  "OcrService": {
    "Host": "http://document-ocr-srv.document-ocr-srv.svc.cluster.local"
  },
  "ActivityService": {
    "Host": "http://activity-api.activity-srv.svc.cluster.local"
  },
  "UserService": {
    "Host": "http://user-api.user.svc.cluster.local",
    "MaxRetry": 3,
    "TimeoutMS": 4000
  },
  "SblService": {
    "Host": "http://set-api.set-srv.svc.cluster.local"
  },
  "Sbl": {
    "InstrumentBucket": "set-sbl-instruments-nonprod"
  },
  "Authentication": {
    "Authority": "https://keycloak.pi.financial/realms/backoffice/",
    "Issuer": "https://keycloak.pi.financial/realms/backoffice/",
    "Audience": "backoffice-admin"
  },
  "OpenTelemetry": {
    "Exporter": {
      "Otlp": {
        "Endpoint": "http://prometheus.pi.financial.nonprod.internal/api/v1/otlp/v1/metrics"
      }
    }
  },
  "CuratedManagerService": {
    "Host": "http://marketdata.nonprod.pi.financial"
  }
}
