{
  "AwsSqs": {
    "Region": "ap-southeast-1"
  },
  "RemoteConfig": {
    "Enable": true,
    "LifetimeMS": 1000,
    "Prefix": "pi/oneport-srv"
  },
  "Features": {
    "Feature1": false
  },
  "MassTransit": {
    "EndpointNamePrefix": "oneport-srv"
  },
  "EnableDbSensitiveDataLogging": false,
  "OpenTelemetry": {
    "Exporter": {
      "Otlp": {
        "Enabled": true,
        "Endpoint": "http://prometheus.pi.financial.nonprod.internal/api/v1/otlp/v1/metrics"
      },
      "Console":{
        "Enabled": false
      }
    }
  },
  "ConnectionStrings": {
    "IFis": "Database={dbName};Server={host}:{port};UID={userId};PWD={password};"
  }
}
